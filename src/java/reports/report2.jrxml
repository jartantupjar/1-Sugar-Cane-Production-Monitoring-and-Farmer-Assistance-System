	<parameter name="batch_num" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select b.batchnum,b.date_issued,s.store_name,s.location,s.contact,bi.model,bi.qty,inv.price
,concat(item.series,'/ ',item.watch_type,'/ ',item.gender,'/ ',item.watch_color) as Description
from batch b join batch_inventory bi on b.batchnum=bi.batchnum 
			 join item item on item.model=bi.model
			join stores s on s.store_num=b.store_num
			join inventory inv on inv.location=b.store_num 
		and  	inv.model=bi.model
where b.batchnum=$P{batch_num}
group by bi.model]]>
	</queryString>
